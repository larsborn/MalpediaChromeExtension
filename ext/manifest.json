{
  "name": "Malpedia",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Enrichment of Websites with content from Malpedia",
  "homepage_url": "https://malpedia.caad.fkie.fraunhofer.de",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/api.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Malpedia",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "https://*/*",
    "http://*/*"
  ]
}